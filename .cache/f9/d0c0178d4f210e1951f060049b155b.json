enter;\n}\n.frame--header {\n  padding: 2rem;\n}\n.frame--footer {\n  padding: 3rem;\n  font-family: Montserrat, system-ui, -apple-system, BlinkMacSystemFont,\n    \"Segoe UI\", Roboto, Oxygen, Ubuntu, Cantarell, \"Open Sans\", \"Helvetica Neue\",\n    sans-serif;\n}\n@media screen and (min-width: 720px) {\n  .frame--footer {\n    padding: 5rem;\n  }\n  .frame--footer h2 {\n    font-size: 3.7rem;\n  }\n}\n\n/* style for header and footer end */\n\n/* Block start */\n\n.block {\n  margin: 8rem 0;\n}\n\n.container {\n  max-width: 960px;\n  margin: 0 auto;\n}\n\n/* Block end */\n\n/* header block start */\n\n.header {\n  position: sticky;\n  top: 0;\n}\n@media screen and (min-width: 960px) {\n  .header h1 {\n    font-size: 4.5rem;\n  }\n}\n\n/* header block end */\n/*# sourceMappingURL=styles.css.map */\n","src\\sass\\_variables.scss":"$color-primary: #085070;\r\n$color-secondary: #f04d64;\r\n$body-text: #1f1f1f;\r\n$standard-version: 960px;\r\n$ipad-version: 720px;\r\n@mixin standard-version() {\r\n  @media screen and (min-width: $standard-version) {\r\n    @content;\r\n  }\r\n}\r\n@mixin ipad-version() {\r\n  @media screen and (min-width: $ipad-version) {\r\n    @content;\r\n  }\r\n}\r\n","src\\sass\\styles.scss":"@import \"./variables\";\r\n@import \"./resets\";\r\n\r\n// Grid start\r\n\r\n.grid {\r\n  display: grid;\r\n  gap: 2rem;\r\n  @include ipad-version() {\r\n    &--1x3 {\r\n      grid-template-columns: repeat(3, 1fr);\r\n    }\r\n    &--1x2 {\r\n      grid-template-columns: repeat(2, 1fr);\r\n    }\r\n  }\r\n}\r\n\r\n// Grid end\r\n// puppy start\r\n\r\n.puppy {\r\n  margin: 0 7%;\r\n  &__missing {\r\n    display: none;\r\n  }\r\n  &__item {\r\n    img {\r\n      width: 100%;\r\n      height: 100%;\r\n    }\r\n  }\r\n  &--header {\r\n    text-align: center;\r\n    margin-bottom: 2rem;\r\n    h2 {\r\n      color: #000;\r\n    }\r\n  }\r\n  @include ipad-version() {\r\n    &__missing {\r\n      align-items: center;\r\n      background-color: $color-secondary;\